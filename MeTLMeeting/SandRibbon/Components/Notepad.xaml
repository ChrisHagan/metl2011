<UserControl x:Class="SandRibbon.Components.Notepad"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:components="clr-namespace:SandRibbon.Components"
    xmlns:core="clr-namespace:System;assembly=mscorlib">
    <UserControl.Resources>
        <core:String x:Key="target">notepad</core:String>
        <core:String x:Key="defaultPrivacy">private</core:String>
    </UserControl.Resources>
    <StackPanel>
        <components:SlideNavigationControls/>
        <components:UserCanvasStack x:Name="stack"></components:UserCanvasStack>
    </StackPanel>
</UserControl>
