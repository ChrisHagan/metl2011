<UserControl x:Class="SandRibbon.Components.PrivateNotepadSpace"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:components="clr-namespace:SandRibbon.Components"
             xmlns:core="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d">
    <UserControl.Resources>
        <core:String x:Key="defaultPrivacy">private</core:String>
        <core:String x:Key="target">notepad</core:String>
    </UserControl.Resources>
    <components:CollapsedCanvasStack x:Name="notepadStack" />
</UserControl>
