<UserControl x:Class="SandRibbon.Frame.Flyouts.DiagnosticsFlyout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SandRibbon.Frame.Flyouts"
             xmlns:layout="clr-namespace:SandRibbon.Layout"
             xmlns:api="clr-namespace:SandRibbon;assembly=SandRibbonInterop"   
             xmlns:osm="http://schemas.awesomium.com/winfx"
             xmlns:viewModel="clr-namespace:SandRibbon.Pages.Conversations.Models"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid 
          layout:GridLayout.ItemsPerRow="1">
        <Label Content="''; logging" />
        <DataGrid ItemsSource="{Binding}" IsReadOnly="True">
            <DataGrid.Foreground>
                <SolidColorBrush Color="{DynamicResource AccentColor}" />
            </DataGrid.Foreground>
        </DataGrid>
    </Grid>
</UserControl>
