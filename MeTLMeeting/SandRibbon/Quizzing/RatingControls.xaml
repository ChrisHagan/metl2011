<UserControl x:Class="SandRibbon.Quizzing.RatingControls"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:interop="clr-namespace:SandRibbonInterop;assembly=SandRibbonInterop"          
    xmlns:quiz="clr-namespace:SandRibbon.Quizzing"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Tag="wormControls">
        <interop:Button Icon="../Resources/ArrowUp.png" CommandParameter="+" Command="{x:Static quiz:RatingControls.RegisterInterest}" />
        <interop:Button Icon="../Resources/ArrowDown.png" CommandParameter="-" Command="{x:Static quiz:RatingControls.RegisterInterest}" />
    </StackPanel>
</UserControl>