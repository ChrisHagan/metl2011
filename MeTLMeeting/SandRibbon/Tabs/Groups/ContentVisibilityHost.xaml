<sr:RibbonGroup x:Class="SandRibbon.Tabs.Groups.ContentVisibilityHost"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sr="clr-namespace:Divelements.SandRibbon;assembly=Divelements.SandRibbon"
    xmlns:components="clr-namespace:SandRibbon.Components"
    xmlns:core="clr-namespace:System;assembly=mscorlib"
    ShowDialogLauncher="False" Header="Content Filter">
    <sr:RibbonGroup.Resources>
        <core:String x:Key="preferredDisplayIndex">8</core:String>
    </sr:RibbonGroup.Resources>
    <sr:RibbonGroup.Variants>
        <sr:GroupVariant Variant="Small" Priority="4"/>
    </sr:RibbonGroup.Variants>
    <StackPanel Orientation="Horizontal">
        <components:ContentVisibility x:Name="contentVisibility" Visibility="Visible"/>
    </StackPanel >
</sr:RibbonGroup>           