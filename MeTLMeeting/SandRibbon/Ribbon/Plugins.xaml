    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    
    xmlns:providers="clr-namespace:SandRibbon.Providers"
    xmlns:sandpit="clr-namespace:SandRibbon.Components.Sandpit"
    xmlns:api="clr-namespace:SandRibbon;assembly=SandRibbonInterop"
    xmlns:core="clr-namespace:System;assembly=mscorlib"
    KeyTip="P" Header="Plugins" x:Name="pluginRoot">
    <RibbonGroup Header="Experimental (Teacher only)" x:Name="teacherPlugins">
        <!--RibbonButton Label="Html" LargeImageSource="\Resources\appbar.form.editable.png" SmallImageSource="\Resources\appbar.form.editable.png" 
                      Command="{x:Static api:Commands.ToggleBrowserControls}" /-->
        <RibbonToggleButton Label="Identifying lens" 
                            Command="{x:Static api:Commands.ToggleLens}"
                            LargeImageSource="\Resources\appbar.image.backlight.png" SmallImageSource="\Resources\appbar.image.backlight.png">
        </RibbonToggleButton>
    </RibbonGroup>
    <RibbonGroup Header="Experimental (Student)">
    </RibbonGroup>
</RibbonTab>