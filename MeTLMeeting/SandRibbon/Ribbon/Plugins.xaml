<RibbonTab x:Class="SandRibbon.Tabs.Plugins"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    
    xmlns:sandpit="clr-namespace:SandRibbon.Components.Sandpit"
    xmlns:api="clr-namespace:SandRibbon;assembly=SandRibbonInterop"
    xmlns:core="clr-namespace:System;assembly=mscorlib"
    KeyTip="P" Header="Plugins" x:Name="pluginRoot">
    <RibbonGroup Header="Experimental">
        <RibbonButton Label="Html" LargeImageSource="\Resources\appbar.form.editable.png" SmallImageSource="\Resources\appbar.form.editable.png" 
                      Command="{x:Static api:Commands.ToggleBrowserControls}" />
    </RibbonGroup>
</RibbonTab>