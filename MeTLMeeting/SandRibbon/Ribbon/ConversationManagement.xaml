<RibbonTab x:Class="SandRibbon.Tabs.ConversationManagement"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:api="clr-namespace:SandRibbon;assembly=SandRibbonInterop"
    xmlns:tabs="clr-namespace:SandRibbon.Tabs"
    xmlns:core="clr-namespace:System;assembly=mscorlib"
    xmlns:dict="clr-namespace:SandRibbon.Components.ResourceDictionaries"
    KeyTip="P" Header="Conversation Management" x:Name="conversationManagement">
    <RibbonGroup Header="">
        <RibbonButton Label="Page Reorder" ToolTip="{DynamicResource PageReorderToolTip}" SmallImageSource="/Resources/NewSlide.png" LargeImageSource="/Resources/NewSlide.png" x:Name="editConversation" 
                     Command="{x:Static api:Commands.EditConversation}" />
        <RibbonToggleButton SmallImageSource="/Resources/Banhammer.png" LargeImageSource="/Resources/Banhammer.png" Label="Administer Content" ToolTip="{DynamicResource BanContentToolTip}" x:Name="banContent" 
                           Command="{x:Static api:Commands.ManageBannedContent}" />
        <RibbonButton Label="Manage banned content" SmallImageSource="/Resources/ban-management-icon.png" LargeImageSource="/Resources/ban-management-icon.png" x:Name="bannedContentManagement" 
                     Command="{x:Static api:Commands.ViewBannedContent}" ToolTip="{DynamicResource BanContentToolTip}"/>
    </RibbonGroup>
</RibbonTab>