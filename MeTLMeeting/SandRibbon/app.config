<?xml version="1.0"?>
<configuration>
	<configSections>
		<sectionGroup name="metlConfigurationGroup">
			<section name="metlConfiguration" type="MeTLLib.MeTLConfigurationSection,MeTLLib"/>
		</sectionGroup>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="SandRibbon.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		</sectionGroup>
	</configSections>

	<startup>
		
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
	<runtime>
		<legacyUnhandledExceptionPolicy enabled="1"/>
	</runtime>
  <metlConfigurationGroup>
    <metlConfiguration>
      <activeStackConfig name="production"/>
     <!-- http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/-->
      <production name="demo" isBootstrapUrl="false" webAuthenticationEndpoint="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/authenticationState" meggleUrl="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/search?query=" thumbnail="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/thumbnail/" protocol="https" host="https://ec2-52-7-180-42.compute-1.amazonaws.com" port="1188" xmppServiceName="metl.adm.monash.edu.au" uploadEndpoint="upload_nested.yaws"/>
      <staging name="monash" isBootstrapUrl="false" webAuthenticationEndpoint="https://ec2-52-7-180-42.compute-1.amazonaws.com:8080/authenticationState" meggleUrl="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/search?query=" thumbnail="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/thumbnail/" protocol="https" host="https://ec2-52-7-180-42.compute-1.amazonaws.com" port="1188" xmppServiceName="metl.adm.monash.edu.au" uploadEndpoint="upload_nested.yaws"/>
      <external name="civic" isBootstrapUrl="false" webAuthenticationEndpoint="https://ec2-52-7-180-42.compute-1.amazonaws.com:8080/authenticationState" meggleUrl="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/search?query=" thumbnail="http://ec2-52-7-180-42.compute-1.amazonaws.com:8080/thumbnail/" protocol="https" host="https://ec2-52-7-180-42.compute-1.amazonaws.com" port="1188" xmppServiceName="metl.adm.monash.edu.au" uploadEndpoint="upload_nested.yaws"/>
      <resourceCredential username="exampleUsername" password="examplePassword"/>
      <xmppCredential username="" password="examplePassword"/>
      <crypto key="01234567" iv="01234567"/>
    </metlConfiguration>
  </metlConfigurationGroup>
  <applicationSettings>
    <SandRibbon.Properties.Settings>
      <setting name="LicenceUrl" serializeAs="String">
        <value>http://metl.adm.monash.edu.au/MeTL/docs/license.html</value>
      </setting>
      <setting name="PrivacyUrl" serializeAs="String">
        <value>http://monash.edu/legals/privacy.html</value>
      </setting>
      <setting name="LegalsUrl" serializeAs="String">
        <value>http://monash.edu/legals/</value>
