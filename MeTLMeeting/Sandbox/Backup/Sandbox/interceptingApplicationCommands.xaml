<UserControl x:Class="Sandbox.interceptingApplicationCommands"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="300" Width="300">
    <UserControl.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Cut" CanExecute="alwaysTrue" Executed="doIT"/>
    </UserControl.CommandBindings>
    <Grid>
        <InkCanvas x:Name="canvas"></InkCanvas>
        <Button Width="10" Height="10"  Click="select"/>
    </Grid>
</UserControl>
